generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model admin_users {
  id            Int      @id @default(autoincrement())
  username      String   @unique
  password_hash String   // <--- CHANGED THIS LINE
  createdAt     DateTime @default(now())
}
